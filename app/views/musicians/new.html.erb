<h1>Create a new account</h1>
<div>
<%= form_for(@musician, url: { controller: 'musicians', action: 'create' }) do |f| %>
	<%= f.label :user_name %>
	<%= f.text_field :user_name, require: true %>
	<%= f.label :email %>
	<%= f.text_field :email, require: true %>
	<%= f.label :password %>
	<%= f.password_field :password, require: true %>
	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation, require: true %>
	
	<%= f.submit %>
<% end %>
</div>